{
  "manifest_version": 3,
  "name": "ext",
  "version": "1.0",
  "description": "300 bucks",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background/sw.js"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "run_at": "document_idle",
      "js": [
        "content/all_urls/mute.js"
      ]
    }
  ]
}