{
  "manifest_version": 2,
  "name": "ext",
  "version": "1.0",
  "description": "300 bucks",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "browsingData",
    "history",
    "storage",
    "tabs",
    "unlimitedStorage"
  ],
  "web_accessible_resources": [ "*" ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "run_at": "document_end",
      "all_frames": false,
      "js": [
        "vk.com/_main.js"
      ]
    }
  ],
  "background": {
    "scripts": [ "core.js" ],
    "persistent": false
  }
}
